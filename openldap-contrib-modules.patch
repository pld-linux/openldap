+
+KRB5_DEFS = -DHAVE_KRB5
+KRB5_LIBS = -lkrb5
+
+RADIUS_LIBS = -lradius
+
+MOD_DEFS = -DSLAPD_IMPORT $(KRB5_DEFS)
+
+shared_LDAP_LIBS = $(LDAP_LIBLDAP_R_LA) $(LDAP_LIBLBER_LA)
